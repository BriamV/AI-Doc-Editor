# Configuración global de registro
registry "https://registry.npmjs.org/"

# Timeouts y configuración de red
network-timeout 600000
HTTP_TIMEOUT 60000

# Forzar el uso de HTTPS para GitHub
"@electron:registry" "https://registry.npmjs.org/"
--install.no-lockfile false
--install.pure-lockfile false

# Usar HTTPS para dependencias Git
ugprade-lockfile false
"--strict-ssl" true
